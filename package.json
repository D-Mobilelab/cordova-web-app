{
  "name": "org.apache.cordova.sampleapp",
  "displayName": "sample-app",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "start:web": "webpack-dev-server --open --config webpack.dev.js",
    "start:hybrid": "webpack-dev-server --env.APP_ENV=hybrid --env.PLATFORM=android --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "cordova-android": "^6.3.0",
    "cordova-ios": "^4.5.3",
    "cordova-plugin-httpd": "^0.9.3",
    "cordova-plugin-whitelist": "^1.3.3",
    "whatwg-fetch": "^2.0.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-httpd": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}